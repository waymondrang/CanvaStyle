{
    "name": "dm4c",
    "version": "0.4.0",
    "description": "dark mode for canvas Â© raymond wang",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "*://canvas.ucsd.edu/*",
                "*://canvas.eee.uci.edu/*",
                "*://canvas.instructure.com/*"
            ],
            "run_at": "document_end",
            "js": [
                "index.js"
            ]
        }
    ],
    "browser_action": {},
    "web_accessible_resources": [
        "dm4c.css"
    ],
    "permissions": [
        "storage"
    ]
}